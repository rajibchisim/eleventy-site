{
  "name": "elevently",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel style:watch eleventy:watch",
    "eleventy:watch": "eleventy --serve",
    "eleventy:build": "ELEVENTY_PRODUCTION=true eleventy",
    "debug": "DEBUG=Eleventy* eleventy:watch && style:watch",
    "style:watch": "postcss ./src/styles/main.css --o ./_tmp/css/main.css --watch",
    "style:build": "NODE_ENV=production postcss ./src/styles/main.css --o ./_tmp/css/main.css",
    "browsersync": "browser-sync start --server '_site' --files '_site' --port 8080 --no-notify --no-open",
    "publish": "npm-run-all style:build eleventy:build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
  },
  "description": "",
  "dependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@tailwindcss/typography": "^0.4.1",
    "autoprefixer": "^10.2.6",
    "cssnano": "^5.0.5",
    "html-minifier": "^4.0.0",
    "js-yaml": "^4.1.0",
    "liquidjs": "^9.25.0",
    "netlify-cli": "^3.30.4",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.0",
    "postcss-cli": "^8.3.1",
    "tailwindcss": "^2.1.2",
    "moment": "^2.29.1",
    "postcss-import": "^14.0.2",
    "@glidejs/glide": "^3.4.1",
    "netlify-cms-app": "^2.15.11"
  }
}
