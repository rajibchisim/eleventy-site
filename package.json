{
  "name": "elevently",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "npm-run-all --parallel css eleventy browsersync",
    "eleventy": "eleventy --watch",
    "debug": "set DEBUG=* & eleventy",
    "css": "postcss src/styles/main.css --o _site/assets/css/main.css --watch",
    "build": "cross-env NODE_ENV=production eleventy && cross-env NODE_ENV=production postcss src/styles/main.css --o _site/assets/css/main.css",
    "browsersync": "browser-sync start --server '_site' --files '_site' --port 8080 --no-notify --no-open"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {},
  "description": "",
  "dependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@glidejs/glide": "^3.4.1",
    "@tailwindcss/typography": "^0.4.1",
    "autoprefixer": "^10.2.6",
    "cross-env": "^7.0.3",
    "cssnano": "^5.0.5",
    "html-minifier": "^4.0.0",
    "html-minifier-terser": "^5.1.1",
    "js-yaml": "^4.1.0",
    "liquidjs": "^9.25.0",
    "moment": "^2.29.1",
    "netlify-cli": "^3.30.4",
    "netlify-cms-app": "^2.15.11",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.0",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^14.0.2",
    "tailwindcss": "^2.1.2"
  }
}
